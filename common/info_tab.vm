<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.2.6/jquery.min.js"></script>


<div id="tabs">
		 <ul class="tab-list">
		#foreach($tab in $infoTabs)
			<li id="$tab.tabId"
			#if($tab.active) 
					class="current" ><a>
			#else
				><a href="$request.contextPath/$tab.tabUrl">
			#end
				
					<span>
						$tab.tabDescription
					</span>
				</a>
			</li>
		#end
	</ul>
</div>
##parse vm detail here
 <div class="tab">
	#foreach($tab in $infoTabs)
		<div id="$tab.tabId" class="tab_content">
			#if($tab.active) 
				<fieldset class="bar" >
				#parse("$tab.tabVmName")
				</fieldset>
			#end
		</div>
	#end
</div>
 
